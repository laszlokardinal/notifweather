SAAA,CAAA,IAAA,IAAA,SAAA,EAAA;AACA,kBAAA,IAAA,CAAA,aAAA,CAAA,YAAA,EACA,IADA,CACA,UAAA,QAAA,EAAA;AACA,oBAAA,KAAA,CAAA,GAAA,GAAA,QAAA,CAAA,IAAA;AACA,oBAAA,MAAA,CAAA,IAAA,CAAA;AAAA,sBAAA,IAAA,EAAA,MAAA;AAAA,sBAAA,MAAA,EAAA;AAAA,wBAAA,IAAA,EAAA,KAAA,CAAA;AAAA;AAAA,qBAAA;AACA,mBAJA,EAKA,KALA,CAKA,UAAA,CAAA,EAAA;AACA,oBAAA,OAAA,CAAA,GAAA,CAAA,CAAA;AACA,mBAPA;AAQA;;AAxBA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAyBA,KA1BA;AA4BA,IAAA,OA5BA,qBA4BA;AACA,WAAA,SAAA,GAAA,KAAA;AACA,WAAA,KAAA,GAAA,EAAA;AACA,WAAA,QAAA,GAAA,EAAA;AACA;AAhCA;AAZA,CAAA","sourcesContent":["<template>\r\n  <div class=\"jumbotron mycustom-jumbotron text-center\">\r\n    <div class=\"submit-form\">\r\n      <div>\r\n        <div class=\"form-group\">\r\n          <h3 class=\"login\">login</h3> <br>\r\n          <h6 class=\"labels\"><label for=\"email\">email</label></h6>\r\n          <input\r\n            type=\"text\"\r\n            class=\"form-control\"\r\n            id=\"email\"\r\n            required\r\n            v-model=\"weather.email\"\r\n            name=\"email\"\r\n          />\r\n          <br>\r\n          <br>\r\n          \r\n          <h6 class=\"labels\"><label for=\"password\">password</label></h6>\r\n          <input\r\n            type=\"text\"\r\n            class=\"form-control\"\r\n            id=\"password\"\r\n            required\r\n            v-model=\"weather.password\"\r\n            name=\"password\"\r\n          />          \r\n        </div>\r\n        <br>\r\n\r\n        <button @click=\"saveInput\" class=\"btn\">submit</button>\r\n      </div>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport Data from \"../services/Data\";\r\nimport router from \"../router\";\r\n\r\nexport default {\r\n  name: \"weather-login\",\r\n  data() {\r\n    return {\r\n      weather: {\r\n        email: \"\",\r\n        password: \"\",\r\n        published: false\r\n      },\r\n      submitted: false\r\n    };\r\n  },\r\n  methods: {\r\n    async saveInput() {\r\n      var data = {\r\n        email: this.weather.email,\r\n        password: this.weather.password,\r\n      };\r\n      var redirectdata = {\r\n        email: this.weather.email\r\n      };\r\n      let authcheck = [];\r\n      try {\r\n        authcheck = await Data.userLogin(data);\r\n      }\r\n      catch {\r\n        alert('Wrong credentials! Please try again.')\r\n      }\r\n      if (authcheck.data != undefined){\r\n        Data.loginRedirect(redirectdata)\r\n          .then(response => {\r\n          this.var = response.data;\r\n          router.push({name: 'home', params: {data: this.var}});\r\n        })\r\n        .catch(e => {\r\n          console.log(e);\r\n        });\r\n      }\r\n    },\r\n    \r\n    newData() {\r\n      this.submitted = false;\r\n      this.email = {};\r\n      this.password = {};\r\n    }\r\n  }\r\n};\r\n</script>\r\n\r\n<style>\r\n.mycustom-jumbotron {\r\n  font-family: 'Montserrat', sans-serif;\r\n  width: 600px;\r\n  height: 500px;\r\n  margin: 0 auto;\r\n  border-radius: 45px;\r\n}\r\n.submit-form {\r\n  max-width: 1000px;\r\n  margin: auto;\r\n}\r\n.labels {\r\n  font-size: 22px;\r\n}\r\n.login {\r\n  font-size: 30px;\r\n}\r\n.btn {\r\n  background-color: #A177FF; \r\n  color: white;\r\n  height: 50px;\r\n  width: 150px;\r\n  font-size: 19.5px;\r\n  text-align: center;\r\n}\r\n</style>"],"sourceRoot":"src/components"}]}