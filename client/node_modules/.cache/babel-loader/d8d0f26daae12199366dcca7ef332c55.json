A;AAAA;;AACA,UAAA,IAAA,GAAA;AACA,QAAA,KAAA,EAAA,KAAA,OAAA,CAAA,KADA;AAEA,QAAA,QAAA,EAAA,KAAA,OAAA,CAAA,QAFA;AAGA,QAAA,MAAA,EAAA,KAAA,OAAA,CAAA,MAHA;AAIA,QAAA,IAAA,EAAA,KAAA,OAAA,CAAA,IAJA;AAKA,QAAA,IAAA,EAAA,KAAA,OAAA,CAAA,IALA;AAMA,QAAA,KAAA,EAAA;AANA,OAAA;AAQA,UAAA,SAAA,GAAA;AACA,QAAA,KAAA,EAAA,KAAA,OAAA,CAAA;AADA,OAAA;AAGA,MAAA,IAAA,CAAA,aAAA,CAAA,SAAA,EACA,IADA,CACA,UAAA,QAAA,EAAA;AACA,QAAA,KAAA,CAAA,GAAA,GAAA,QAAA,CAAA,IAAA;AACA,OAHA;;AAIA,UAAA,KAAA,GAAA,CAAA,CAAA,CAAA,IAAA,SAAA,EAAA;AACA,QAAA,KAAA,CAAA,qBAAA,CAAA;AACA,OAFA,MAGA;AACA,YAAA,WAAA,CAAA,KAAA,OAAA,CAAA,KAAA,CAAA,IAAA,KAAA,EAAA;AACA,UAAA,KAAA,CAAA,oCAAA,CAAA;AACA,SAFA,MAGA;AAEA,cAAA,YAAA,CAAA,KAAA,OAAA,CAAA,MAAA,CAAA,IAAA,KAAA,EAAA;AACA,YAAA,KAAA,CAAA,2CAAA,CAAA;AACA;AACA;;AAEA,QAAA,IAAA,CAAA,UAAA,CAAA,IAAA,EACA,IADA,CACA,UAAA,QAAA,EAAA;AACA,UAAA,KAAA,CAAA,OAAA,CAAA,EAAA,GAAA,QAAA,CAAA,IAAA,CAAA,EAAA;AACA,UAAA,OAAA,CAAA,GAAA,CAAA,QAAA,CAAA,IAAA;AACA,UAAA,KAAA,CAAA,SAAA,GAAA,IAAA;AACA,UAAA,IAAA,CAAA,sBAAA,CAAA,IAAA;AACA,SANA,EAOA,KAPA,CAOA,UAAA,CAAA,EAAA;AACA,UAAA,OAAA,CAAA,GAAA,CAAA,CAAA;AACA,SATA;AAUA;AACA,KA1CA;AA2CA,IAAA,OA3CA,qBA2CA;AACA,WAAA,SAAA,GAAA,KAAA;AACA,WAAA,KAAA,GAAA,EAAA;AACA,WAAA,QAAA,GAAA,EAAA;AACA,WAAA,MAAA,GAAA,EAAA;AAEA,KAjDA;AAkDA,IAAA,QAlDA,sBAkDA;AACA,MAAA,OAAA,CAAA,GAAA,CAAA,OAAA;AACA,MAAA,MAAA,CAAA,IAAA,CAAA;AAAA,QAAA,IAAA,EAAA;AAAA,OAAA;AACA;AArDA;AAhBA,CAAA","sourcesContent":["<template>\r\n  <div class=\"jumbotron alerts text-center\">\r\n    <div class=\"submit-form\">\r\n      <div v-if=\"!submitted\">\r\n        <div class=\"form-group\">\r\n          <h6><label for=\"email\">Email</label></h6>\r\n          <input\r\n            type=\"text\"\r\n            class=\"form-control\"\r\n            id=\"email\"\r\n            required\r\n            v-model=\"weather.email\"\r\n            name=\"email\"\r\n          />\r\n          <h6><label for=\"password\">Password:</label></h6>\r\n          <input\r\n            type=\"text\"\r\n            class=\"form-control\"\r\n            id=\"password\"\r\n            required\r\n            v-model=\"weather.password\"\r\n            name=\"password\"\r\n          />          \r\n          <h6><label for=\"number\">What is your phone number to receive push notifications?</label></h6>\r\n          <input\r\n            type=\"text\"\r\n            class=\"form-control\"\r\n            id=\"number\"\r\n            required\r\n            v-model=\"weather.number\"\r\n            name=\"number\"\r\n          />\r\n          <h6><label for=\"number\">What city would you like to receive notifications for?</label></h6>\r\n          <input\r\n            type=\"text\"\r\n            class=\"form-control\"\r\n            id=\"number\"\r\n            required\r\n            v-model=\"weather.city\"\r\n            name=\"number\"\r\n          />\r\n          <h6><label for=\"time\">What time would you like to be notified?</label></h6>\r\n          <select\r\n            type=\"text\"\r\n            class=\"form-control\"\r\n            id=\"time\"\r\n            required\r\n            v-model=\"weather.time\"\r\n            name=\"time\"\r\n          >\r\n              <option value=\"1\">1 AM</option>\r\n              <option value=\"2\">2 AM</option>\r\n              <option value=\"3\">3 AM</option>\r\n              <option value=\"4\">4 AM</option>\r\n              <option value=\"5\">5 AM</option>\r\n              <option value=\"6\">6 AM</option>\r\n              <option value=\"7\">7 AM</option>\r\n              <option value=\"8\">8 AM</option>\r\n              <option value=\"9\">9 AM</option>\r\n              <option value=\"10\">10 AM</option>\r\n              <option value=\"1q\">11 AM</option>\r\n              <option value=\"12\">12 AM</option>\r\n              <option value=\"13\">1 PM</option>\r\n              <option value=\"14\">2 PM</option>\r\n              <option value=\"15\">3 PM</option>\r\n              <option value=\"16\">4 PM</option>\r\n              <option value=\"17\">5 PM</option>\r\n              <option value=\"18\">6 PM</option>\r\n              <option value=\"19\">7 PM</option>\r\n              <option value=\"20\">8 PM</option>\r\n              <option value=\"21\">9 PM</option>\r\n              <option value=\"22\">10 PM</option>\r\n              <option value=\"23\">11 PM</option>\r\n              <option value=\"24\">12 PM</option>\r\n          </select>\r\n        </div>\r\n\r\n        <button @click=\"saveInput\" class=\"btn btn-success\">Submit</button>\r\n      </div>\r\n      <div v-else>\r\n        <h4>Account Created!</h4>\r\n        <button class=\"btn btn-success\" @click=\"redirect\">Login</button>\r\n      </div>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport Data from \"../services/Data\";\r\nimport router from \"../router\";\r\n\r\nfunction verifyEmail(email){\r\n      const re = /^(([^<>()[\\]\\\\.,;:\\s@\"]+(\\.[^<>()[\\]\\\\.,;:\\s@\"]+)*)|(\".+\"))@((\\[[0-9]{1,3}\\.[0-9]{1,3}\\.[0-9]{1,3}\\.[0-9]{1,3}\\])|(([a-zA-Z\\-0-9]+\\.)+[a-zA-Z]{2,}))$/;\r\n      return re.test(String(email).toLowerCase());\r\n}\r\nfunction verifyNumber(number){\r\n      const re = /^[+]?[(]?[0-9]{3}[)]?[-\\s.]?[0-9]{3}[-\\s.]?[0-9]{4,6}$/im;\r\n      return re.test(String(number));\r\n}\r\n\r\nexport default {\r\n  name: \"weather-alerts\",\r\n  data() {\r\n    return {\r\n      weather: {\r\n        id: null,\r\n        email: \"\",\r\n        password: \"\",\r\n        number: \"\",\r\n        city: \"\",\r\n        time: \"\",\r\n        published: false\r\n      },\r\n      submitted: false\r\n    };\r\n  },\r\n  methods: {\r\n    saveInput() {\r\n      var data = {\r\n        email: this.weather.email,\r\n        password: this.weather.password,\r\n        number: this.weather.number,\r\n        time: this.weather.time,\r\n        city: this.weather.city,\r\n        state: true\r\n      };\r\n      var authemail = {\r\n        email: this.weather.email\r\n      };\r\n      Data.loginRedirect(authemail)\r\n        .then(response => {\r\n        this.var = response.data;\r\n        })\r\n      if ((this.var[0]) != undefined) {\r\n        alert('Email already used!');\r\n      }\r\n      else {\r\n        if (verifyEmail(this.weather.email) == false) {\r\n          alert('Email not valid! Please try again.');\r\n        }\r\n        else{\r\n\r\n          if (verifyNumber(this.weather.number) == false) {\r\n            alert('Phone Number not valid! Please try again.');\r\n          }\r\n        }\r\n\r\n        Data.createUser(data)\r\n          .then(response => {\r\n            this.weather.id = response.data.id;\r\n            console.log(response.data);\r\n            this.submitted = true;\r\n            Data.subscribeNotifications(data);\r\n          })\r\n          .catch(e => {\r\n            console.log(e);\r\n          });\r\n      }    \r\n    },\r\n    newData() {\r\n      this.submitted = false;\r\n      this.email = {};\r\n      this.password = {};\r\n      this.number = {};\r\n      \r\n    },\r\n    redirect(){\r\n      console.log('hello');\r\n      router.push({name: 'login'});\r\n    }\r\n  }\r\n};\r\n</script>\r\n\r\n<style>\r\n.alerts {\r\n  border:10px solid #003366;\r\n  width: 800px;\r\n  margin: 0 auto;\r\n}\r\n.submit-form {\r\n  max-width: 300px;\r\n  margin: auto;\r\n}\r\n</style>"],"sourceRoot":"src/components"}]}